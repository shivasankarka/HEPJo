DataTypes module for short hand data type aliases
"""

alias i8 = DType.int8
alias i16 = DType.int16
alias i32 = DType.int32
alias i64 = DType.int64
alias isize = DType.index
alias u8 = DType.uint8
alias u16 = DType.uint16
